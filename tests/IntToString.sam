  function run(): unit = {
    val _ = ForTests.printlnInt(0);
    val _ = ForTests.printlnInt(1);
    val _ = ForTests.printlnInt(123);
    val _ = ForTests.printlnInt(1234567);
    val _ = ForTests.printlnInt(2147483647);
