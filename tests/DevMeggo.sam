class Dev(Sam(unit), Megan(string)) {
  private method getName(): string =
    match this {
      Sam(_) -> "sam",
      Megan(n) -> n,
    }

  function meggo(): Dev = Dev.Megan("meggo")

  method vibez(): unit = Builtins.println("dev" :: this.getName() :: "vibez")
}

class DevMeggo {
  function run(): unit = Dev.meggo().vibez()
}
