import { ForTests } from tests.StdLib

class AndOrInsideIf {
  private function test(): int = {
    val i: int = 1;
    val j: int = 2;
    if (i < j /*  */ && i > 0 && /* f */ j > 0) then {
      val a: int = 3;
      val b: int = 4;
      if (a > b || a + b > 0 && true) then 1 else 2
    } else {
      val a: int = 3;
      val b: int = 4;
      if (a == 2 || b == 4) then { 3 } else { 4 }
    }
  }

  function run(): unit = ForTests.assertIntEquals(1, AndOrInsideIf.test())
}
