class PrintHelloWorld {
  private function globalConstantTest(): unit = {
    let a1 = "a";
    let a2 = "a";
    if a1 == a2 {  } else { Process.panic("") }
  }

  private function printTest(): unit = {
    let h = "Hello";
    let w = " World!";
    Process.println(h :: w)
  }

  function run(): unit = {
    PrintHelloWorld.globalConstantTest();
    PrintHelloWorld.printTest();
  }
}
