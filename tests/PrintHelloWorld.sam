class PrintHelloWorld {
  private function globalConstantTest(): unit = {
    let a1 = "a";
    let a2 = "a";
    if a1 == a2 then {  } else Process.panic("")
  }

  private function printTest(): unit = {
    let h = "Hello";
    let w = " World!";
    Process.println(h :: w)
  }

  function run(): unit = {
    let _ = PrintHelloWorld.globalConstantTest();
    let _ = PrintHelloWorld.printTest();
  }
}
