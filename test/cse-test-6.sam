class Main {
  function printInt(i: int): unit = Builtins.println(Builtins.intToString(i))

  function check(actual: int, expected: int): unit =
    if (actual != expected) then Builtins.panic(
      "actual: " :: Builtins.intToString(actual) :: ", expected " :: Builtins.intToString(expected)
    ) else {  }

  function test(first: bool, a: int, b: int, aTimesB: int): unit = {
    val _: unit = if (first) then {
      val _: int = a * b;
    } else {  };
    val _: unit = Main.check(a * b, aTimesB);
  }

  function main(): unit = {
    val _: unit = Main.test(true, 3, 4, 12);
    val _: unit = Main.test(false, 3, 4, 12);
    Builtins.println("OK")
  }

}
