class Main {
  function mathTailRec(n: int, acc: int): int =
    if (n == 0) then acc else Main.mathTailRec(n - 1, acc + stringToInt(intToString(1)))

  function main(): unit = {
    val actual = Main.mathTailRec(100000, 0);
    if (actual == 100000) then println("Yay") else panic(intToString(actual))
  }

}
