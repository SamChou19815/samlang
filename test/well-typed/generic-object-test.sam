class GenericObject<T1, T2>(val v1: T1, val v2: T2) {
    function main(): unit = {
        val f = (v2) -> (
            if (v2 + 1 == 3) then
                { v1: 3, v2 }
            else
                { v1: 3, v2: 42 }
        );
        val _ = println(intToString(f(2).v2)); // print 2
        val _ = println(intToString(f(3).v2)); // print 42
    }
}

class Main {
    function main(): unit = GenericObject.main()
}
