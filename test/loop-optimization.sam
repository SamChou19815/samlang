class Main {
  function printInt(n: int): unit = Builtins.println(Builtins.intToString(n))

  function loopy(i: int): int =
    if (i >= 10) then 0 else {
      val j: int = i * 3 + 100;
      val _: unit = Main.printInt(j);
      Main.loopy(i + 2)
    }

  function main(): unit = {
    val _: int = Main.loopy(0);
  }

}
