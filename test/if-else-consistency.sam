class Main {
  function main(): unit = {
    val a: int = if (true) then (if (false) then 10000 else 3) else 4;
    val b: int = if (false) then 4 else if (true) then 3 else 20000;
    val _: unit = Builtins.println(Builtins.intToString(a));
    val _: unit = Builtins.println(Builtins.intToString(b));
    if (a != b) then Builtins.panic("Not OK") else Builtins.println("OK")
  }

}
