{
  "name": "vscode-samlang",
  "displayName": "vscode-samlang",
  "description": "SAMLANG for VSCode",
  "version": "0.0.3",
  "publisher": "dev-sam",
  "repository": {
    "url": "https://github.com/SamChou19815/samlang"
  },
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "icon": "images/samlang.png",
  "main": "./out/src/extension",
  "activationEvents": [
    "onLanguage:samlang"
  ],
  "contributes": {
    "languages": [
      {
        "id": "samlang",
        "aliases": [
          "SAMLANG",
          "samlang"
        ],
        "extensions": [
          ".sam"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "samlang",
        "scopeName": "text.samlang",
        "path": "./syntaxes/samlang.json"
      }
    ],
    "snippets": [
      {
        "language": "samlang",
        "path": "./snippets.json"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "SAMLANG Language Service Configuration",
      "properties": {
        "samlang.programPath": {
          "type": "string",
          "description": "Path to `samlang` program."
        }
      }
    }
  },
  "scripts": {
    "postinstall": "tsc"
  },
  "devDependencies": {
    "@types/node": "^13.1.0",
    "@types/vscode": "1.41.0",
    "typescript": "3.7.4",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "vscode-languageclient": "^5.2.1"
  }
}
