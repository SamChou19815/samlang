{
  "`val` expression": {
    "prefix": "val",
    "body": ["val $1 = $2;", "$3"],
    "description": "Define a variable."
  },
  "object class": {
    "prefix": "oclass",
    "body": ["class $1($2: $3) {$4}"],
    "description": "A class backed by objects."
  },
  "variant class": {
    "prefix": "vclass",
    "body": ["class $1($2($3), $4($5)) {$6}"],
    "description": "A class backed by variants."
  },
  "utility class": {
    "prefix": "uclass",
    "body": ["class $1 {}"],
    "description": "A utility class."
  }
}
