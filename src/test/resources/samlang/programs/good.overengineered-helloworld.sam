class HelloWorld(val message: string) {
    method getMessage(): string = {
        val { message } = this;
        message
    }

    public function getGlobalMessage(): string = {
        val hw = { message: "Hello World!" };
        hw.getMessage()
    }
}

class Main {
    function main(): string = HelloWorld.getGlobalMessage()
}
