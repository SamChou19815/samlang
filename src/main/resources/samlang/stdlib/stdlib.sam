/**
 * Useful for function references and basic operations.
 */
class Pervasives {
    function plus(a: int, b: int): int = a + b
    function minus(a: int, b: int): int = a - b
    function multiply(a: int, b: int): int = a * b
    function divide(a: int, b: int): int = a / b
    function mod(a: int, b: int): int = a % b
    function readLine(): string = "DUMMY_LINE"
    function println(content: string): unit = unit
}
